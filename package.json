{
  "name": "youtube_streaming_watcher",
  "scripts": {
    "start:notify": "NODE_ENV=development node --unhandled-rejections=strict dist/dev/notify.js",
    "start:reply": "NODE_ENV=development node --unhandled-rejections=strict dist/dev/reply.js",
    "build": "webpack && npm-pack-zip",
    "build:dev": "npm run build:dev:notify && npm run build:dev:reply",
    "build:dev:notify": "NODE_ENV=development NODE_OPTIONS='--max-old-space-size=2048' webpack -c webpack.config.dev.notify.js",
    "build:dev:reply": "NODE_ENV=development webpack -c webpack.config.dev.reply.js",
    "fix": "eslint --fix **/*.ts"
  },
  "files": [
    "dist/lambda",
    "schema"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "@slack/bolt": "^3.9.0",
    "@slack/web-api": "^6.6.0",
    "bufferutil": "^4.0.6",
    "googleapis": "^95.0.0",
    "sleep-promise": "^9.1.0",
    "utf-8-validate": "^5.0.8"
  },
  "bundledDependencies": [
    "express"
  ],
  "devDependencies": {
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.1",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "npm-pack-zip": "^1.3.0",
    "textlint": "^12.1.0",
    "textlint-rule-abbr-within-parentheses": "^1.0.2",
    "textlint-rule-footnote-order": "^1.0.3",
    "textlint-rule-general-novel-style-ja": "dev-hato/textlint-rule-general-novel-style-ja-markdown",
    "textlint-rule-ja-hiragana-fukushi": "^1.3.0",
    "textlint-rule-ja-hiragana-hojodoushi": "^1.0.4",
    "textlint-rule-ja-hiragana-keishikimeishi": "^1.1.0",
    "textlint-rule-ja-unnatural-alphabet": "^2.0.1",
    "textlint-rule-ng-word": "^1.0.0",
    "textlint-rule-no-dead-link": "^4.8.0",
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": "^1.0.1",
    "textlint-rule-prefer-tari-tari": "^1.0.3",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-preset-jtf-style": "^2.3.12",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^14.19.0",
    "npm": "^7.21.0"
  }
}
