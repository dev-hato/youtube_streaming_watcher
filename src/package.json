{
  "name": "youtube_streaming_watcher",
  "scripts": {
    "start:notify": "NODE_ENV=development node --unhandled-rejections=strict dist/dev/notify.js",
    "start:reply": "NODE_ENV=development node --unhandled-rejections=strict dist/dev/reply.js",
    "build": "webpack && npm-pack-zip",
    "build:dev": "npm run build:dev:notify && npm run build:dev:reply",
    "build:dev:notify": "NODE_ENV=development webpack -c webpack.config.dev.notify.js",
    "build:dev:reply": "NODE_ENV=development webpack -c webpack.config.dev.reply.js",
    "fix": "eslint --resolve-plugins-relative-to . --fix src/**/*.ts"
  },
  "files": [
    "dist/lambda"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "@slack/bolt": "^3.9.0",
    "@slack/web-api": "^6.6.0",
    "@vendia/serverless-express": "^4.5.3",
    "bufferutil": "^4.0.6",
    "googleapis": "^95.0.0",
    "sleep-promise": "^9.1.0",
    "utf-8-validate": "^5.0.8"
  },
  "bundledDependencies": [
    "express"
  ],
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "npm-pack-zip": "^1.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "~3.9.7",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^14.19.0",
    "npm": "^7.21.0"
  }
}
